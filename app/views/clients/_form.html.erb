<%= simple_form_for(@client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :copay %>
    <%= f.input :effective_on %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
